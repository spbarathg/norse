{
  "name": "norse",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc -p tsconfig.json && node -e \"const fs=require('fs'),path=require('path');const mk=(p)=>{try{fs.mkdirSync(p,{recursive:true})}catch{} };mk('dist/config');mk('dist/data');fs.cpSync(path.join('src','config'),'dist/config',{recursive:true});fs.cpSync('data','dist/data',{recursive:true});\"",
    "start": "node dist/index.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name init",
    "prisma:studio": "prisma studio",
    "prisma:seed": "tsx prisma/seed.ts",
    "decay:tick": "tsx -e \"import('./src/engines/decay.ts').then(m=>m.runDecayTick().then(console.log)).catch(console.error)\"",
    "register-commands": "tsx src/bot/registerCommands.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.18.0",
    "cors": "^2.8.5",
    "discord.js": "^14.16.3",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "seedrandom": "^3.0.5",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.12",
    "prisma": "^5.18.0",
    "tsx": "^4.16.2",
    "typescript": "^5.5.4"
  }
} 